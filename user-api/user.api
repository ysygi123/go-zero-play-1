type (
	UserReq {
		Id int `path:"id"`
	}

	UserReply {
		Id   int    `json:"id"`
		Name string `json:"name"`
	}

	JwtReply {
		Token  string `json:"token"`
		Expire int64  `json:"expire"`
	}
)
@server(
	jwt: Auth
	middleware: TestMiddleware1
)
service user {
	@handler getUser
	get /api/user/get/:id (UserReq) returns (UserReply)
}

service user {
	@handler getJWT
	get /api/user/jwt returns (JwtReply)
}